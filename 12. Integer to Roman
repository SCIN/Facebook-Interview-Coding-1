12. Integer to Roman

string intToRoman(int num) {
    vector<string> sym = {"M", "CM", "D", "CD", "C", "XC", "L", "XL", "X", "IX", "V", "IV", "I"};
    vector<int> nums = {1000, 900, 500, 400, 100, 90, 50, 40, 10, 9, 5, 4, 1};
    string ret = "";
    for (int i = 0; i < 13; i++) {
        while (num >= nums[i]) {
            num -= nums[i];
            ret += sym[i];
        }
    }
    return ret;
}

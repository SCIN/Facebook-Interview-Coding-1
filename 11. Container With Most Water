11. Container With Most Water

int maxArea(vector<int>& height) {
    int left = 0;
    int right = height.size() - 1;
    int area = 0;
    while (left < right) {
        area = max(area, min(height[left], height[right]) * abs(left - right));
        if (height[left] < height[right]) {
            left++;
        } else {
            right--;
        }
    }
    return area;
}
